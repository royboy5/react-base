{
  "presets": ["@babel/preset-env", "@babel/preset-react"],
  "plugins": [
    [
      "module-resolver",
      {
        "extensions": [".js", ".jsx", ".css"],
        "alias": {
          "@Components": "./src/components",
          "@Pages": "./src/pages",
          "@Styles": "./src/styles"
        }
      }
    ]
  ]
}